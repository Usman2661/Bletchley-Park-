<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_admin_homepage"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.usman.bletcheypark.adminHomepage">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="22dp"
            android:layout_marginTop="54dp"
            android:id="@+id/textView2" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button33"
            android:layout_alignTop="@+id/button32"
            android:layout_alignParentEnd="true"
            android:background="@drawable/admintwo" />

        <Button
            android:id="@+id/register"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignStart="@+id/button33"
            android:layout_below="@+id/button33"
            android:background="?attr/colorAccent"
            android:text="Track User Activity" />

        <Button
            android:text="POST EVENTS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/events"
            android:background="@color/colorAccent"
            android:layout_below="@+id/button32"
            android:layout_alignParentStart="true"
            android:layout_alignEnd="@+id/button32" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button32"
            android:background="@drawable/events"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true" />

        <Button
            android:id="@+id/viewFeedback"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/leavefeedback"
            android:layout_below="@+id/events"
            android:layout_alignParentStart="true"
            android:layout_marginTop="73dp" />

        <Button
            android:id="@+id/createAdmin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/rsz_createadmin"
            android:layout_alignBaseline="@+id/viewFeedback"
            android:layout_alignBottom="@+id/viewFeedback"
            android:layout_alignParentEnd="true" />

        <Button
            android:id="@+id/button24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/colorAccent"
            android:text="Create Admin"
            android:layout_below="@+id/createAdmin"
            android:layout_alignParentEnd="true"
            android:layout_alignStart="@+id/createAdmin" />

        <Button
            android:id="@+id/button4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/colorAccent"
            android:text="View Feedback"
            android:layout_below="@+id/createAdmin"
            android:layout_alignParentStart="true"
            android:layout_alignEnd="@+id/viewFeedback" />

    </RelativeLayout>

</RelativeLayout>




